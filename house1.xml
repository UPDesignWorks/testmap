	<action name="closemap">
		<!--设定下一次onclick的动作-->
        set(onclick, openmap() );
        <!-- 改变XY坐标，将导航图容器移出屏幕，移出过程不设置，即为默认0.5秒-->
        tween(layer[huxing1].x,0,0.5);
		 tween(layer[huxing1].y,0,0.5);
     </action>
    <action name="openmap">
		<!--设定下一次onclick的动作-->
        set(onclick, closemap() );
       <!-- 改变XY坐标，将导航图容器移入屏幕，移入过程为1秒-->
        tween(layer[huxing1].x,750,0.5);
		tween(layer[huxing1].y,0,0.5);
    </action>
	

